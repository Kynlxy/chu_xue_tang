<template name="swiperBar">
    <view class="banner-div">
        <swiper  autoplay="true"
                interval="5000" duration="1000" bindanimationfinish="endFunction" >
            <block wx:for="{{imgUrls}}"  wx:key="imgUrls" data-index="{{index}}">
                <swiper-item>
                    <image src="{{item}}"/>
                </swiper-item>

            </block>

        </swiper>
    </view>

</template>