page{
    background-color: #faf5f5;
}
.main-center-div-warp{
    padding: 35rpx 4% 45rpx 4%;
    background: #ffffff;
}
.tip-font-div{
    color: #666666;
    line-height: 40rpx;
}
.common-img-div{
    width: 600rpx;
    height: 400rpx;
    border-radius: 20rpx;
    margin: 40rpx auto 0 auto;
}
.common-img-div image{
    width: 100%;
    height: 100%;
    border-radius: 20rpx;
}
.distinguish-result{
    padding: 35rpx 4%;
    font-size: 32rpx;
    font-weight: bold;
}
.common-line-div-warp{
    background: #ffffff;
}
.common-line-div{
    position: relative;
    width: 92%;
    padding: 20rpx 4% 20rpx 0;
    height: 50rpx;
    line-height: 50rpx;
    margin-left: 4%;
    overflow: hidden;
}
.common-line-div input {
    padding-left: 20rpx;
}

.border-bottom{
    border-bottom: 1px solid #eeeeee;
}
.publish-warp{
    margin-top: 20rpx;
    width: 100%;
    padding: 35rpx 0;
    border-radius: 10rpx;
    text-align: center;
    background: #ffffff;
    z-index: 11111;
    font-size: 36rpx;
}
.btn-submit{
    width: 92%;
    line-height: 60rpx;
    text-align: center;
    color: #ffffff;
    background: #379949;
    padding: 10rpx 0;
    margin: 0 auto;
    font-size: 32rpx;
    border-radius: 10rpx;
}
.position-right-choose{
    position:absolute;
    top:20rpx;
    right:4%;
    width:57%;
    padding-right:4%;
    height:40rpx;
    text-align:right;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
    overflow: hidden;
}
.position-right-choose .path-div{
    position:absolute;
    right:0;
    top:10rpx;
    width:17rpx;
    height:30rpx;

}