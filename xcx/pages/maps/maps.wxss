/* pages/maps/maps.wxss */
page{
    background-color: #f6f6f6;
}
.main-warp{
    padding-bottom: 100rpx !important;
}
.main-top-div{
    position: relative;
    display: flex;
    width: 100%;
    height: 100rpx;
    background:#f1921f ;
}
.common-div{
    padding: 0 20rpx;
    flex: 1;
    height: 130rpx;
    color: #ffffff;
    line-height: 80rpx;
    font-size: 26rpx;
}
#map{
    position: relative;
    width: 100%;
    height: 900rpx;
}
.btn-get-place{
    width: 80%;
    padding: 20rpx 0;
    margin: 50rpx auto 0 auto;
    text-align: center;
    background: #08ac08;
    border-radius: 10rpx;
    color: #ffffff;
}
.tips-font{
    padding: 20rpx 10% 0 10%;
    font-size: 28rpx;
    line-height:40rpx;
}
.main-cover-warp{
    position: fixed;
    top: 0;
    bottom: 0;
    right: 0;
    left: 0;
    background: rgba(0, 0, 0, 0.5);
    z-index: 1111;
}
.main-center-div{
    padding: 30rpx 4%;
    width: 70%;
    height: 180rpx;
    position: absolute;
    top: 0;
    bottom: 0;
    right: 0;
    left: 0;
    margin: auto;
    background: #ffffff;
    border-radius: 10rpx;
}
.last-btn-div{
    position:absolute;
    height:60rpx;
    display:flex;
    bottom:15rpx;
    width:100%;
    left:0;
}
.common-btn{
    flex: 1;
    line-height: 70rpx;
    text-align: center;
}
.ok-btn{
    color: #08ac08;
}
.main-show{
    padding: 10rpx;
    overflow: auto;
    white-space:normal;
}